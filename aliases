alias Less=less
alias Ls=ls
alias Sort=sort
alias amke=make
alias maek=make
alias mkae=make
alias cdws='cd $HOME/ws/$(date +%Y%m%d)'
alias cdwy='cd "$HOME/ws/"`if [ ! -d "$HOME/ws/$(date +%Y%m%d)" ]; then mkws && ls $HOME/ws/$(date +%Y%m%d)/../ | tail -n 2 | head -n 1 ; else ls $HOME/ws/$(date +%Y%m%d)/../ | tail -n 2 | head -n 1; fi`'
alias clena=clean
alias diff='diff -u'
alias dir=ls
alias dirs='dirs -l -p -v'
alias echopath="echo $PATH | sed 's/:/\n/g'"
alias gitk='gitk --all'
alias givm=gvim
alias got=go
alias grep='egrep -Ea --exclude={cscope.out,tags}'
alias gvimr='gvim --remote'
alias h=history
alias l=ls
alias less='less -X'
alias ll='ls -l'
alias ls='ls -F'
alias rm=trash
alias sl=ls
alias srot=sort
alias ssh-start='eval `ssh-agent`'
alias ssh='ssh -AY'
alias vi=vim
alias xml_pp='xml_pp -v -l -s indented'
alias scren=screen
alias adsf=asdf
