alias Sort=sort
alias amke=make
alias maek=make
alias mkae=make
alias cdgo='cd $GOPATH'
alias cdws='cd $HOME/ws/$(date +%Y%m%d)'
alias cdwy='cd "$HOME/ws/"`if [ ! -d "$HOME/ws/$(date +%Y%m%d)" ]; then mkws && ls $HOME/ws/$(date +%Y%m%d)/../ | tail -n 2 | head -n 1 ; else ls $HOME/ws/$(date +%Y%m%d)/../ | tail -n 2 | head -n 1; fi`'
alias clena=clean
alias diff='diff -u'
alias dir=ls
alias dirs='dirs -l -p -v'
alias echopath="echo $PATH | sed 's/:/\n/g'"
alias gitk='/bin/rm -f ~/.config/git/gitk && gitk --all'
alias givm=gvim
alias got=go
alias gvimr='gvim --remote'
alias h=history
alias jq='jq -C'
alias jshell='jshell --feedback verbose --startup DEFAULT --startup PRINTING --class-path $(jars.sh) $*'
alias k=kubectl
alias kx=kubectx
alias l=ls
alias less='less -X'
alias ll='ls -l'
alias ls='ls -F'
alias rm=trash
alias sl=ls
alias srot=sort
alias ssh-start='eval `ssh-agent`'
alias ssh='ssh -AY'
